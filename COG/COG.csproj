    <Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <DebugType>embedded</DebugType>
        
        <VersionPrefix>1.0.0</VersionPrefix>
        <VersionSuffix>dev</VersionSuffix>
        <Description>A mod for Among Us</Description>
        <Authors>commandf1</Authors>
        <Nullable>enable</Nullable>
        <AssemblyName>ClashOfGods</AssemblyName>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="Resources\InDLL\Images\Setting\Crewmate.png" />
      <None Remove="Resources\InDLL\Images\Setting\Imposter.png" />
      <None Remove="Resources\InDLL\Images\Setting\Neutral.png" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Include="Resources\InDLL\Images\COG-BG.png" />
      <EmbeddedResource Include="Resources\InDLL\Images\Setting\*.*" />
    </ItemGroup>
        
    <ItemGroup>
      <EmbeddedResource Include="Resources\InDLL\Config\*.*" />
      <EmbeddedResource Include="Resources\InDLL\Depends\*.*" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Reactor" Version="2.1.0" />
        <PackageReference Include="BepInEx.Unity.IL2CPP" Version="6.0.0-be.663" Private="false" ExcludeAssets="runtime;native" />
        <PackageReference Include="AmongUs.GameLibs.Steam" Version="2023.7.11" PrivateAssets="all" />

        <PackageReference Include="BepInEx.AutoPlugin" Version="1.1.0" PrivateAssets="all" />
        <PackageReference Include="BepInEx.IL2CPP.MSBuild" Version="2.0.1" PrivateAssets="all" ExcludeAssets="runtime" />
        <PackageReference Include="YamlDotNet" Version="13.1.1" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="GitInfo" Version="2.3.0" />
        <PackageReference Include="BepInEx.AutoPlugin" Version="1.0.1" PrivateAssets="all" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Role\Impl\Crewmate\" />
      <Folder Include="Role\Impl\Impostor\" />
    </ItemGroup>

    <Target Name="Copy" AfterTargets="Build" Condition="'$(AmongUs)' != ''">
        <Copy SourceFiles="$(TargetPath)" DestinationFolder="$(AmongUs)/BepInEx/plugins/" UseSymboliclinksIfPossible="true" />
    </Target>
</Project>
